	 process(clk, reset)
    
	 begin
        if rising_edge(clk) then
            
				if reset = '1' then
				
					parity_bits_out <= (others => '0');
					
				else
				
				parity_bits_out <= parity_bits;
				
				message_out <= mes_delay & parity_bits;
				
			
				
				
				end if;
			
				
			end if;
    end process;
	 
end architecture ;



