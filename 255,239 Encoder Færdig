library ieee;
use ieee.std_logic_1164.all;
use ieee.numeric_std.all;

entity bch_encoder is
port (
		clk : in  STD_LOGIC;
		reset : in  STD_LOGIC;
		message : in  STD_LOGIC_VECTOR (0 to 238);
		parity_bits : out STD_LOGIC_VECTOR (0 to 15)
);
end entity bch_encoder;

